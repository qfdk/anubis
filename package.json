  },
  "dependencies": {
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "fail2ban": "git+https://github.com/drudrum/fail2ban.git",
    "geoip-lite": "^1.4.5",
    "http-errors": "~1.6.3",
    "log4js": "^6.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "pkg": "^5.6.0"
  }
}

{
  "name": "anubis",
  "version": "0.0.0",
  "private": true,
  "bin": "./bin/www",
  "pkg": {
    "targets": [
      "node18-linux",
      "node18-macos"
    ],
    "assets": [
      "public/**/*",
      "views/**/*"
    ]
  },
  "scripts": {
    "build": "pkg . --out-path=dist",
    "dev": "nodemon ./bin/www",
    "start": "node ./bin/www",
    "update": "wget https://github.com/out0fmemory/qqwry.dat/raw/master/qqwry_lastest.dat -O ./data/qqwry_lastest.dat"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "fail2ban": "git+https://github.com/drudrum/fail2ban.git",
    "geoip-lite": "^1.4.7",
    "http-errors": "~1.6.3",
    "log4js": "^6.9.1",
    "serve-favicon": "^2.5.0"
  },
  "optionalDependencies": {
    "compression": "^1.7.4",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "pkg": "^5.8.1"
  }
}
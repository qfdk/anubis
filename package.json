{
  "name": "anubis",
  "version": "0.0.0",
  "private": true,
  "bin": "./bin/www",
  "pkg": {
    "targets": [
      "node16-linux",
      "node16-macos"
    ],
    "assets": [
      "public/**/*",
      "views/**/*"
    ]
  },
  "scripts": {
    "build": "pkg . --out-path=dist",
    "start": "nodemon ./bin/www",

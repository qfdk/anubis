# 基本服务配置
PORT=1233
BASE_PATH=
LOG_LEVEL=info

# 管理员账户设置
USERNAME=admin
PASSWORD=admin

# 开发/生产模式切换
# true表示使用模拟数据，无需真实fail2ban
# false表示使用真实fail2ban数据
IS_MOCK=true

# Fail2Ban配置（使用真实fail2ban时需要）
FAIL2BAN_SOCKET_PATH=/var/run/fail2ban/fail2ban.sock
FAIL2BAN_JAIL_PATH=/etc/fail2ban/jail.d
FAIL2BAN_FILTER_PATH=/etc/fail2ban/filter.d

# 安全设置
SESSION_SECRET=change-this-to-a-random-string
JWT_SECRET=change-this-to-another-random-string
<%- include('../includes/header'); %>
    <div class="jumbotron">
        <h2>监狱列表</h2>
        <p>当前监狱个数【<%- jails; %>】</p>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">
            监狱列表
        </div>
        <table class="table">
            <thead>
                <tr>
                    <th>#</th>
                    <th>监狱名称</th>
                    <th>操作</th>
                </tr>
            </thead>
            <tbody>
                <% for(let i=0;i < list.length;i++){ %>
                    <tr>
                        <th scope="row">
                            <%- i+1; %>
                        </th>
                        <td>
                            <%- list[i]; %>
                        </td>
                        <td>
                            <a class="btn btn-xs btn-info" href="/admin/jails/<%- list[i]; %>"><i
                                    class="fa fa-search" aria-hidden="true"></i> 查看</a>
                        </td>
                    </tr>
                    <% } %>
            </tbody>
        </table>
    </div>
    <%- include('../includes/footer'); %>
<%- include('../includes/header'); %>
<div class="jumbotron">
    <h2>菜单管理</h2>
    <p>菜单管理设置</p>
</div>
<div class="panel panel-default">
    <div class="panel-heading">菜单管理</div>
    <div class="panel-body">
        <form action="/admin/menu/doEdit" method="POST" class="form-horizontal">
            <p class="alert alert-info">请用","作为分隔符，最好不超过 <b>5</b> 个</p>
            <div class="form-group">
                <div class="col-md-12">
                    <input type="text" class="form-control" name="menus" value="<%=menus%>">
                </div>
            </div>
            <div class="form-group" style="text-align:center">
                <input type="submit" class="btn btn-primary">
            </div>
        </form>
    </div>
</div>
<%- include('../includes/footer'); %>
